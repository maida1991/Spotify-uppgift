pipeline {
    agent any
     
    stages{
         stage("build") {
            steps {
               echo 'makeing My First Playlista on Spotify'
            }
          }
      }
      stages{
         stage("testning") {
            steps {
            echo 'testning My Playlista on Spotify'
            }
          }
      }
      stages{
         stage("deploy") {
            steps {
            echo 'deploying My First Playlista on Spotify'
            }
          }
      }
}
            
