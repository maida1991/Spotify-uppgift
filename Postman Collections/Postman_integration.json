                  "tests[\"Status code is 200\"] = (responseCode.code === 200);\r",
                  "\r",
                  "if (responseCode.code === 200) {\r",
                  "    var artist = JSON.parse(responseBody);\r",
                  "    // Store the artist name in the environment\r",
                  "    postman.setEnvironmentVariable(\"artist_name\", artist.name);\r",
                  "    // Store the artist id in an artists array\r",
                  "    postman.setEnvironmentVariable(\"artists\", JSON.stringify([environment.id]));\r",
                  "    // As defined in the workflow, set the next request to be executed to Get Related Artists\r",
                  "    postman.setNextRequest(\"Get Related Artists\");\r",
                  "    console.log(\"Information retrieved for artist: \" + environment.artist_name);\r",
